{
  "[python]": {
    "editor.formatOnType": true,
    "editor.formatOnSave": true
  },
  "files.autoSave": "afterDelay",
  "jupyter.themeMatplotlibPlots": true,
  "jupyter.widgetScriptSources": ["unpkg.com", "jsdelivr.com"],
  "files.exclude": {
    "**/.csv": true,
    "**/.parquet": true,
    "**/.pkl": true,
    "**/.xls": true
  },
  "editor.fontSize": 18,
  "notebook.output.scrolling": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "right"
  },
  "python.languageServer": "Pylance",
  "workbench.editorLargeFileConfirmation": 40,
  "markdown.preview.fontSize": 20,
  "redhat.telemetry.enabled": true,
  "notebook.formatOnCellExecution": true,
  "notebook.formatOnSave.enabled": true,
  "chatgpt.lang": "en",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "diffEditor.ignoreTrimWhitespace": false
  },
  "cSpell.userWords": ["Jiahao", "Xiang"],
  "editor.fontFamily": "'Martian Mono', Consolas, 'Courier New', monospace",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.lineNumbers": "relative",
  "github.copilot.chat.editor.temporalContext.enabled": true,
  "notebook.markup.fontFamily": "Consolas",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "breadcrumbs.enabled": false,
  "notebook.stickyScroll.enabled": true,
  "github.copilot.chat.notebook.followCellExecution.enabled": true,
  "python.analysis.typeCheckingMode": "basic",
  "explorer.confirmDelete": false
}
